<?xml version='1.0' encoding='UTF-8'?>
<collectionReaderDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <implementationName>de.julielab.jcore.reader.iexml.IEXMLFileReader</implementationName>
  <processingResourceMetaData>
    <name>JCoRe IEXML Reader</name>
    <description />
    <version>2.6.0</version>
        <vendor>JULIE Lab Jena, Germany</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>inputFile</name>
        <description>the Mantra xml input file</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>maxRecordsCount</name>
        <description>Non-mandatory parameter that allows you to read only a limited number of records into the CAS.</description>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>inputFile</name>
        <value>
          <string>/enter/your/path/here</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="de.julielab.jcore.types.jcore-all-types" />
      </imports>
    </typeSystemDescription>
    <typePriorities />
    <fsIndexCollection />
    <capabilities>
      <capability>
        <inputs />
        <outputs>
          <type allAnnotatorFeatures="true">de.julielab.jcore.types.mantra.Unit</type>
          <type allAnnotatorFeatures="true">de.julielab.jcore.types.mantra.Entity</type>
          <type allAnnotatorFeatures="true">de.julielab.jcore.types.mantra.DocumentInformation</type>
        </outputs>
        <languagesSupported />
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>true</outputsNewCASes>
    </operationalProperties>
  </processingResourceMetaData>
  <resourceManagerConfiguration />
</collectionReaderDescription>