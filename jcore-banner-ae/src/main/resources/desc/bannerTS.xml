<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>bannerTS</name>
  <description>basic typesystem started by sid</description>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>banner.types.uima.Gene</name>
      <description>Just marks the gene</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description>id is the reference to a standard database such as uniprot or ensembl	
</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>banner.types.uima.Source</name>
      <description>stores information about the source of the input</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>filePath</name>
          <description>file path of the source</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>text</name>
          <description>text in the file or line</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
