    <url>scm:git:https://github.com/JULIELab/jcore-base</url>
  </scm>
  <distributionManagement>
    <repository>
      <id>julie-nexus</id>
      <name>JULIE Lab Releases</name>
      <url>https://www.coling.uni-jena.de/nexus/content/repositories/releases/</url>
    </repository>
    <snapshotRepository>
      <id>julie-nexus</id>
      <name>JULIE Lab Snapshots</name>
      <url>https://www.coling.uni-jena.de/nexus/content/repositories/snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <jcore-utilities-version>2.3.3-SNAPSHOT</jcore-utilities-version>
    <uima-version>2.10.2</uima-version>
    <jcore-version>2.3.0</jcore-version>
    <julielab-java-utilities-version>1.0.4</julielab-java-utilities-version>
    <maven.compiler.target>1.8</maven.compiler.target>
    <slf4j-version>1.7.25</slf4j-version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <uimafit-version>2.4.0</uimafit-version>
    <guava-version>27.0.1-jre</guava-version>
    <jcore-parent-version>2.3.4-SNAPSHOT</jcore-parent-version>
    <gpg.executable>gpg</gpg.executable>
    <logback-version>1.2.3</logback-version>
    <additionalparam>-Xdoclint:none</additionalparam>
    <uima-ducc-version>2.2.2</uima-ducc-version>
    <jcore-types-version>2.3.4-SNAPSHOT</jcore-types-version>
    <gpg.passphrase>fiioagrarwissenschaftskapitaen</gpg.passphrase>
    <maven.compiler.source>1.8</maven.compiler.source>
  </properties>
  <dependencies>
    <dependency>
      <groupId>aopalliance</groupId>
      <artifactId>aopalliance</artifactId>
      <version>1.0</version>
      <scope>compile</scope>
    </dependency>
