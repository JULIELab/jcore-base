<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>JCoRe Mantra Types</name>
  <description>The type system contains types for working with documents in the context of the MANTRA project.</description>
  <version>2.0.0-SNAPSHOT</version>
  <vendor>JULIE Lab Jena, Germany</vendor>
  <imports>
    <import location="../jcore-document-structure-types.xml"/>
    <import location="../jcore-basic-types.xml"/>
    <import location="../jcore-document-meta-types.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>de.julielab.jcore.types.mantra.Corpus</name>
      <description>Stores Information about the corpus.</description>
      <supertypeName>de.julielab.jcore.types.DocumentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>creationDate</name>
          <description>Date this corpus was created.</description>
          <rangeTypeName>de.julielab.jcore.types.Date</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>id</name>
          <description>ID of the corpus file this document belongs to.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>docType</name>
          <description>Type of the documents in this corpus, e.g. Medline(title)</description>
          <rangeTypeName>de.julielab.jcore.types.mantra.DocType</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>language</name>
          <description>The language of this corpus</description>
          <rangeTypeName>de.julielab.jcore.types.mantra.Language</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.mantra.Document</name>
      <description>Stores Information about this document.</description>
      <supertypeName>de.julielab.jcore.types.DocumentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description>ID of the document</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.mantra.Unit</name>
      <description>Unit in a Mantra document (could be e.g. a sentence or
				paragraph, depending on the alignment level of the corpus).</description>
      <supertypeName>de.julielab.jcore.types.Zone</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description>the id of the document</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>text</name>
          <description>the text of this unit</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.mantra.Entity</name>
      <description>Used to annotate entities (called e in the xsd)</description>
      <supertypeName>de.julielab.jcore.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>source</name>
          <description>Ontology which provided this annotation.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>cui</name>
          <description>UMLS cui for the term.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semanticType</name>
          <description>Semantic type of this term, may contain several whitespace separated entries.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semanticGroup</name>
          <description>Semantic group this term belongs to.</description>
          <rangeTypeName>de.julielab.jcore.types.mantra.SemanticGroup</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>ner</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>de.julielab.jcore.types.mantra.NER</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.mantra.DocType</name>
      <description>Type/Source of this corpus</description>
      <supertypeName>uima.cas.String</supertypeName>
      <allowedValues>
        <value>
          <string>EMEA</string>
          <description/>
        </value>
        <value>
