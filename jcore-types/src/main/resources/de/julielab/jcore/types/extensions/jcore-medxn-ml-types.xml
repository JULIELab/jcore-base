<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>jcore-medxn-ml-types</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <imports>
    <import name="de.julielab.jcore.types.extensions.jcore-medxn-syntactic-types"/>
  </imports>
  <types>
    <typeDescription>
      <name>de.julielab.jcore.types.ohnlp.shareAnnotation</name>
      <description/>
      <supertypeName>de.julielab.jcore.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>annotType</name>
          <description>corresponding to mentionClass</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>annotValue</name>
          <description>corresponding to mentionValue</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>annotSlots</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>de.julielab.jcore.types.ohnlp.shareSlot</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.ohnlp.i2b2Event</name>
      <description/>
      <supertypeName>de.julielab.jcore.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>eventType</name>
          <description>occurrence, evidential, test, problem, treatment or clinical_dept</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginLineToken</name>
          <description>index of the tokens in the line, where the event starts starts with 0</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endLineToken</name>
          <description>index of the tokens in the line, where the event ends</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginLine</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endLine</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.ohnlp.i2b2Token</name>
      <description/>
      <supertypeName>de.julielab.jcore.types.ohnlp.WordToken</supertypeName>
      <features>
        <featureDescription>
          <name>lineNumber</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lineTokenNumber</name>
          <description>index of the token on the line beginning from 0</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>i2b2Begin</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>i2b2End</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>capitalization</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.ohnlp.shareToken</name>
      <description/>
      <supertypeName>de.julielab.jcore.types.Token</supertypeName>
      <features>
        <featureDescription>
          <name>lineNumber</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lineTokenNumber</name>
          <description>index of the token on the line begining from 0</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>shareBegin</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>shareEnd</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>capitalization</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>de.julielab.jcore.types.ohnlp.shareSlot</name>
      <description/>
      <supertypeName>de.julielab.jcore.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>slotClass</name>
          <description>The corresponding semantic classes for the slot</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>annotBegin</name>
          <description>The corresponding annotation begins</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>annotEnd</name>
          <description>The corresponding annotation ends</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>slotConceptValue</name>
          <description>The corresponding normalized concept values for the slots</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>slotStringValue</name>
          <description>Normalized String Value</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  
  </types>
</typeSystemDescription>
