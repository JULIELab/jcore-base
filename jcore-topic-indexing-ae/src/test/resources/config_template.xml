<?xml version="1.0" encoding="UTF-8" ?>
<tmconfig>
	<model>
		<meta>
			<ID>UnitTest</ID>
			<Version>0.0</Version>
		</meta>	
	</model>
	<train>
		<parameters>
			<parameter>
				<numTopics>100</numTopics>
			</parameter>
			<parameter>
				<alphaSum>1.0</alphaSum>
			</parameter>
			<parameter>
				<beta>0.1</beta>
			</parameter>
			<parameter>
				<numThreads>1</numThreads>
			</parameter>
			<parameter>
				<numIterations>200</numIterations>
			</parameter>
			<parameter>
				<optimizationInterval>50</optimizationInterval>
			</parameter>
		</parameters>
	</train>
	<search>
		<parameters>
			<parameter>
				<probabilityThreshold>0.01</probabilityThreshold>
			</parameter>
		</parameters>
	</search>
	<infer>
		<parameters>
			<parameter>
				<numIterations>500</numIterations>
			</parameter>
			<parameter>
				<savingInterval>10</savingInterval>
			</parameter>
			<parameter>
				<firstSavingInterval>50</firstSavingInterval>
			</parameter>
		</parameters>
	</infer>
</tmconfig>